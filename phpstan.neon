includes:
    - ./vendor/nunomaduro/larastan/extension.neon

parameters:

    paths:
        - app
        - config
        - database/factories
        - database/seeders
        - routes
        - tests

    ignoreErrors:
        -
            message: '#Undefined variable: \$this#'
            paths:
                - tests/*

    # Level 9 is the highest level
    level: 8

    checkMissingIterableValueType: false
